<?xml version="1.0" encoding="utf-8"?>
<odoo>
       <data>
         <record model="ir.ui.view" id="pc_view_product_template_form_ext">
            <field name="name">Product template Form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
             <xpath expr="//field[@name='seller_ids']" position="inside">
                <form string="Vendor Information">
                  <group name="studio_group_833569">
                    <group name="vendor" string="Vendor">
                      <field name="sequence" string="Secuencia de Proveedores"/>
                      <field name="product_variant_count" invisible="1"/>
                      <field name="name" context="{'res_partner_search_mode': 'supplier'}"/>
                      <field name="product_name"/>
                      <field name="product_code"/>
                      <field name="product_id" groups="product.group_product_variant" domain="[('product_tmpl_id', '=', product_tmpl_id)]" options="{'no_create_edit': True}"/>
                      <label for="delay"/>
                      <div name="studio_div_4553ec">
                        <field name="delay" class="oe_inline"/> days
                                        </div>
                    </group>
                    <group string="Price List" name="studio_group_f2f2f0">
                      <field name="product_tmpl_id" string="Product" invisible="context.get('visible_product_tmpl_id', True)"/>
                      <label for="min_qty"/>
                      <div class="o_row" name="studio_div_490ad7">
                        <field name="min_qty"/>
                        <field name="product_uom" groups="uom.group_uom"/>
                      </div>
                      <label for="price"/>
                      <div class="o_row" name="studio_div_ce41e3">
                        <field name="price"/>
                        <field name="currency_id" groups="base.group_multi_currency"/>
                      </div>
                      <label for="date_start" string="Validity"/>
                      <div class="o_row" name="studio_div_ab6235">
                        <field name="date_start"/> to <field name="date_end"/>
                      </div>
                    </group>
                    <group string="Other Information" groups="base.group_multi_company" name="studio_group_c67da0">
                      <field name="company_id" options="{'no_create': True}"/>
                    </group>
                  </group>
                  <notebook name="studio_notebook_6rHG2">
                    <page name="studio_page_6rHG2" string="Observaciones">
                      <group name="studio_group_6rHG2">
                        <field name="x_minimo_notas" string="Obs. 1"/>
                        <field name="x_studio_obs_2_2" string="Obs. 2"/>
                      </group>
                    </page>
                  </notebook>
                </form>
                <tree string="Vendor Information" multi_edit="1">
                  <field name="sequence" widget="handle" readonly="1"/>
                  <field name="name" readonly="1"/>
                  <field name="product_id" optional="show" invisible="context.get('product_template_invisible_variant', False)" groups="product.group_product_variant" attrs="{}" readonly="1"/>
                  <field name="product_code" optional="show"/>
                  <field name="product_tmpl_id" string="Product" readonly="1" invisible="context.get('visible_product_tmpl_id', True)"/>
                  <field name="currency_id" groups="base.group_multi_currency" attrs="{}"/>
                  <field name="date_start" optional="hide"/>
                  <field name="date_end" optional="hide"/>
                  <field name="company_id" readonly="1" groups="base.group_multi_company"/>
                  <field name="min_qty"/>
                  <field name="product_uom" groups="uom.group_uom"/>
                  <field name="price" string="Price"/>
                  <field name="x_minimo_notas" string="Obs. 1"/>
                  <field name="x_studio_obs_2_2"/>
                </tree>
              </xpath>

            </field>
        </record>


       </data>
</odoo>