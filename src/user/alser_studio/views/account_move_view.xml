<?xml version="1.0" encoding="utf-8"?>
<odoo>
       <data>

        <record model="ir.ui.view" id="pc_view_account_move_tree_ext">
            <field name="name">Fecha de cobro Tree</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="arch" type="xml">
                <data>
                     <xpath expr="//tree[1]/field[@name='invoice_partner_display_name']" position="after">
                            <field name="x_studio_fecha_de_cobro"/>
                      </xpath>
                </data>
            </field>
        </record>

              <record model="ir.ui.view" id="pc_view_account_move_form_ext">
            <field name="name">Fecha de cobro Form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='invoice_date']" position="after">
                      <field name="x_studio_fecha_de_cobro" string="Fecha de cobro"/>
                     </xpath>
                </data>
            </field>
        </record>



       </data>
</odoo>
